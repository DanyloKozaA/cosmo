type Mutation{
    createTransaction(date:String, business: String, price: Float, debit: Float, credit: Float, valueDate: String, balance: Float, TitleId: String): Transaction
}

type Transaction {
    date: String
    business: String
    price: Float
    debit: Float
    credit: Float
    valueDate: String
    balance: Float
    TitleId: String
}

type Query {
    sort(encode: String): String
}




